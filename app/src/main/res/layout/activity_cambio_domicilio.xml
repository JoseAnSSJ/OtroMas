<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:layout_width="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="match_parent">
    <!--Primeros datos-->
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="1000dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/cambio_Domicilio"
                android:layout_centerHorizontal="true"
                android:textSize="25dp"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/ciudadDos"
                android:textSize="20dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="85dp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginTop="85dp"
                android:textSize="20dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="75dp"
                android:id="@+id/ciudadCAMDO"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/localidadDos"
                android:textSize="20dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="115dp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginTop="115dp"
                android:textSize="20dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="100dp"
                android:id="@+id/localidad"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/coloniaDos"
                android:textSize="20dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="145dp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginTop="145dp"
                android:textSize="20dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="75dp"
                android:id="@+id/colonia"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/calleDos"
                android:textSize="20dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="175dp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginTop="175dp"
                android:textSize="20dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="60dp"
                android:id="@+id/Calledom"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/numeroDos"
                android:textSize="20dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="205dp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginTop="205dp"
                android:textSize="20dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="75dp"
                android:id="@+id/numero"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/numeroIDos"
                android:textSize="20dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="235dp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginTop="235dp"
                android:textSize="20dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="150dp"
                android:id="@+id/numeroi"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/telefonoDos"
                android:textSize="20dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="265dp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginTop="265dp"
                android:textSize="20dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="90dp"
                android:id="@+id/telefono"
                />
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="350dp"
                android:orientation="horizontal"
                android:layout_alignTop="@id/telefono"
                android:layout_marginTop="50dp"
                android:id="@+id/map"
                >
                <RelativeLayout
                    android:id="@+id/uno"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@drawable/contorno"
                    >
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/dos"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@drawable/contorno"
                    android:layout_below="@id/uno"
                    android:layout_marginTop="50dp"
                    >
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@drawable/contorno"
                    android:layout_below="@id/dos"
                    android:layout_marginTop="50dp"
                    >
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="110dp"
                    android:layout_marginEnd="110dp"
                    android:id="@+id/center"
                    >
                    <RelativeLayout
                        android:id="@+id/cuatro"
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:background="@drawable/contorno"
                        android:layout_centerHorizontal="true"
                        >
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/cinco"
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:background="@drawable/contorno"
                        android:layout_below="@id/cuatro"
                        android:layout_marginTop="50dp"
                        android:layout_centerHorizontal="true">
                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:src="@drawable/casa"
                            android:layout_centerHorizontal="true"
                            android:id="@+id/casanorte"
                            android:visibility="invisible"
                            />
                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:src="@drawable/casa"
                            android:layout_centerHorizontal="true"
                            android:layout_alignParentBottom="true"
                            android:id="@+id/casasur"
                            android:visibility="invisible"
                            />
                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:src="@drawable/casa"
                            android:layout_centerVertical="true"
                            android:id="@+id/casaoeste"
                            android:visibility="invisible"
                            />
                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:src="@drawable/casa"
                            android:layout_centerVertical="true"
                            android:layout_alignParentEnd="true"
                            android:id="@+id/casaeste"
                            android:visibility="invisible"
                            />
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:background="@drawable/contorno"
                        android:layout_below="@id/cinco"
                        android:layout_marginTop="50dp"
                        android:layout_centerHorizontal="true"
                        >
                    </RelativeLayout>
                    <TextView
                        android:id="@+id/callenorte"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/calle_Norte"
                        android:layout_alignParentTop="true"
                        android:layout_marginTop="100dp"
                        android:layout_centerHorizontal="true"
                        />
                    <TextView
                        android:id="@+id/callesur"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/calle_Sur"
                        android:layout_alignParentTop="true"
                        android:layout_marginTop="220dp"
                        android:layout_centerHorizontal="true"
                        />
                </RelativeLayout>
                <TextView
                    android:id="@+id/calleeste"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:text="@string/calle_Este"
                    android:rotation="90"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="60dp"
                    />
                <TextView
                    android:id="@+id/calleoeste"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:text="@string/calle_Oeste"
                    android:layout_centerVertical="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="40dp"
                    android:rotation="90"
                    />
                <RelativeLayout
                    android:id="@+id/siete"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@drawable/contorno"
                    android:layout_alignParentEnd="true"
                    >
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/ocho"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@drawable/contorno"
                    android:layout_alignParentEnd="true"
                    android:layout_below="@id/siete"
                    android:layout_marginTop="50dp"
                    >
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@drawable/contorno"
                    android:layout_alignParentEnd="true"
                    android:layout_below="@id/ocho"
                    android:layout_marginTop="50dp"
                    >
                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/flecha"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        />
                </RelativeLayout>
            </RelativeLayout>

            <Button
                android:id="@+id/aceptar"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/map"
                android:layout_alignParentStart="true"
                android:layout_marginStart="67dp"
                android:layout_marginTop="18dp"
                android:background="@drawable/boton"
                android:backgroundTint="@color/colorPrimary"
                android:text="@string/aceptar" />

            <Button
                android:id="@+id/regresa"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/map"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="252dp"
                android:layout_marginTop="18dp"
                android:layout_marginEnd="59dp"
                android:background="@drawable/boton"
                android:backgroundTint="@color/colorPrimary"
                android:text="@string/Regresar" />
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>