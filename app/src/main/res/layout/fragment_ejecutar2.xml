<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.Ejecutar1Fragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/ejecutarR"
            android:layout_width="121dp"
            android:layout_height="58dp"
            android:layout_alignParentStart="true"
            android:layout_marginStart="30dp"
            android:layout_marginTop="150dp"
            android:background="@drawable/boton"
            android:backgroundTint="@color/colorPrimary"
            android:text="@string/ejecutar"
            android:textColor="#000" />

        <Button
            android:id="@+id/firmarRep"
            android:layout_width="121dp"
            android:layout_height="58dp"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="17dp"
            android:layout_marginTop="150dp"
            android:layout_marginEnd="27dp"
            android:layout_toEndOf="@+id/ejecutarR"
            android:background="@drawable/boton"
            android:backgroundTint="@color/colorPrimary"
            android:enabled="true"
            android:text="@string/firmar"
            android:textColor="#000" />

        <Button
            android:id="@+id/SalirR"
            android:layout_width="121dp"
            android:layout_height="58dp"
            android:layout_below="@+id/ejecutarR"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20dp"
            android:background="@drawable/boton"
            android:backgroundTint="@color/colorPrimary"
            android:text="@string/salir"
            android:textColor="#000" />
    </RelativeLayout>
</FrameLayout>