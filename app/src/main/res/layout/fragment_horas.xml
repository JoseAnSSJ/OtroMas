<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.HorasFragment">
    <RelativeLayout
        android:id="@+id/rb"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="89dp">
        <RadioGroup
            android:id="@+id/grupo_dias"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">
            <RadioButton
                android:id="@+id/ejutada1"
                android:layout_width="82dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="80dp"
                android:clickable="false"
                android:duplicateParentState="false"
                android:text="@string/visita"
                android:visibility="visible" />
            <RadioButton
                android:id="@+id/visitada1"
                android:layout_width="107dp"
                android:layout_height="wrap_content"
                android:checked="true"
                android:clickable="false"
                android:focusable="auto"
                android:text="@string/ejecutada"
                android:visibility="visible"
                />
        </RadioGroup>
    </RelativeLayout>
    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="120dp"
        android:verticalScrollbarPosition="defaultPosition"
        android:visibility="visible">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="375dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true">
            <RelativeLayout
                android:id="@+id/RE1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="0dp"
                tools:ignore="NotSibling">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="18dp"
                    android:text="@string/ejecucion"
                    android:textSize="20dp" />

                <EditText
                    android:id="@+id/ejecureal1"
                    android:layout_width="224dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="136dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="51dp"
                    android:focusable="false"
                    android:hint="@string/fechaformato"
                    android:inputType="date"
                    android:textSize="20dp" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/RV1"
                android:layout_width="match_parent"
                android:layout_height="193dp"
                android:layout_below="@+id/RE1"
                android:visibility="gone"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp"
                    android:text="@string/visita1"
                    android:layout_marginStart="10dp"

                    android:textSize="22sp"
                    android:textStyle="bold"
                    />
                <TextView
                    android:id="@+id/visit2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="70dp"
                    android:text="@string/visita2"
                    android:layout_marginStart="10dp"
                    android:textSize="22sp"
                    android:textStyle="bold"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="130dp"
                    android:layout_marginStart="10dp"
                    android:text="@string/visita3"
                    android:textSize="22sp"
                    android:textStyle="bold" />
                <EditText
                    android:id="@+id/visita11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="200dp"
                    android:hint="@string/fechaformato"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="15dp"
                    android:focusable="false"
                    android:textSize="20dp" />
                <EditText
                    android:id="@+id/visita21"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/visita11"
                    android:hint="@string/fechaformato"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="200dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="15dp"
                    android:enabled="false"
                    android:focusable="false"
                    android:textSize="20dp" />
                <EditText
                    android:id="@+id/visita31"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/visita21"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:hint="@string/fechaformato"

                    android:layout_marginStart="200dp"
                    android:layout_marginTop="10dp"
                    android:enabled="false"
                    android:layout_marginEnd="15dp"
                    android:focusable="false"
                    android:textSize="20dp"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="376dp"
                android:layout_height="105dp"
                android:layout_below="@+id/RV1"
                android:layout_alignParentStart="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="20dp">
                <!-- <TextView
                     android:id="@+id/horabi"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_alignParentStart="true"
                     android:layout_marginStart="10dp"
                     android:hint="@string/formatoHora"

                     android:text="@string/horai"
                     android:textSize="20dp" />
                 <TextView
                     android:id="@+id/horabf"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_alignParentStart="true"
                     android:layout_alignParentBottom="true"
                     android:layout_marginStart="10dp"
                     android:layout_marginTop="50dp"
                     android:layout_marginBottom="20dp"
                     android:text="@string/horaf"
                     android:hint="@string/formatoHora"
                     android:textSize="20dp" />

                 <EditText
                     android:id="@+id/horaf11"
                     android:layout_width="150dp"
                     android:layout_height="44dp"
                     android:layout_alignParentStart="true"
                     android:layout_alignParentTop="true"
                     android:layout_alignParentEnd="true"
                     android:layout_alignParentBottom="true"
                     android:layout_marginStart="135dp"
                     android:layout_marginTop="52dp"
                     android:layout_marginEnd="91dp"
                     android:layout_marginBottom="9dp"
                     android:focusable="false"
                     android:hint="00:00"
                     android:textSize="20dp" />

                 <EditText
                     android:id="@+id/horai11"
                     android:layout_width="118dp"
                     android:layout_height="44dp"
                     android:layout_alignParentStart="true"
                     android:layout_alignParentTop="true"
                     android:layout_alignParentBottom="true"
                     android:layout_marginStart="136dp"
                     android:layout_marginTop="0dp"
                     android:layout_marginBottom="61dp"
                     android:focusable="false"
                     android:hint="00:00"
                     android:textSize="20dp" />-->
            </RelativeLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginStart="10dp"
                android:layout_marginTop="400dp"
                android:text="@string/tecnico_Sec"
                android:textSize="20dp" />
            <Spinner
                android:id="@+id/tecnicosec1"
                android:layout_width="match_parent"
                android:layout_height="28dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="432dp"
                android:background="@drawable/disenio_spinner"/>
        </RelativeLayout>
    </ScrollView>
</FrameLayout>