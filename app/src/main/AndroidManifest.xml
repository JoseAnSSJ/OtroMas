<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pablo.prueba7">
    <uses-permission android:name="android.permission.INTERNET"/>
        <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
        <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION">



    </uses-permission>

    <application
        android:largeHeap="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activitys.Inicio"
            android:label="Inicio"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="orientation|keyboardHidden|screenSize|screenLayout"
            android:screenOrientation="portrait"
            >

        </activity>
        <activity
            android:name=".Activitys.Orden"
            android:label="@string/title_activity_orden"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="orientation|keyboardHidden|screenSize|screenLayout"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Activitys.Reportes"
            android:label="@string/title_activity_reportes"
            android:theme="@style/AppTheme.NoActionBar"
            />
        <activity
            android:name=".Activitys.Configuracion"
            android:label="@string/title_activity_configuracion"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="orientation|keyboardHidden|screenSize|screenLayout"
            android:screenOrientation="portrait"
            ></activity>

        <activity android:name=".Activitys.MainActivity"
            android:configChanges="orientation|keyboardHidden|screenSize|screenLayout"
            ></activity>
        <activity android:name=".Activitys.asignacion"
            android:theme="@style/AppTheme.NoActionBar"

            ></activity>
        <activity android:name=".Activitys.asignado"
            android:theme="@style/AppTheme.NoActionBar"

            ></activity>
        <activity android:name=".Activitys.ExtensionesAdi"
            android:theme="@style/AppTheme.NoActionBar"

            ></activity>
        <activity android:name=".Activitys.MainReportes"
            android:configChanges="screenSize|orientation"
            ></activity>

        <activity android:name=".Activitys.Login"
            android:theme="@style/AppTheme.NoActionBar"

            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activitys.CambioDom"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="screenSize|orientation"
            ></activity>
        <activity android:name=".Activitys.CambioAparato"
            android:theme="@style/AppTheme.NoActionBar"></activity>

    </application>

</manifest>